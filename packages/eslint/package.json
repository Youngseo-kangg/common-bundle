  "description": "Youngseo-kangg's eslint custom plugin",
  "repository": {
    "type": "git",
    "url": "https://github.com/Youngseo-kangg/common-bundle",
    "directory": "packages/eslint"
  },
  "license": "MIT",
  "type": "commonjs",
  "author": {
    "name": "Youngseo Kang",
